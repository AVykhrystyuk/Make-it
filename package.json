{
  "name": "make-it-app",
  "version": "1.0.0",
  "description": "MakeIt app offers useful and user-friendly features to organize your tasks and optimize your productivity.",
  "scripts": {
    "start": "npm run dev-server",
    "dev-server": "webpack-dev-server --progress --inline --config ./config/webpack.config.js",
    "dev-server-prod": "set NODE_ENV=production && webpack-dev-server --progress --inline --config ./config/webpack.config.js",
    "build": "rimraf build && webpack --progress --display-modules --config ./config/webpack.config.js",
    "build-prod": "rimraf build && set NODE_ENV=production && webpack --display-modules --config ./config/webpack.config.js",
    "serve": "node server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "license": "ISC",
  "dependencies": {
    "angular": "^1.6.1",
    "bootstrap": "^3.3.7",
    "express": "^4.15.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.0",
    "babel-preset-es2015": "^6.24.0",
    "css-loader": "^0.27.3",
    "eslint": "^3.17.1",
    "eslint-loader": "^1.6.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "less": "^2.7.2",
    "less-loader": "^3.0.0",
    "postcss-loader": "^1.3.3",
    "rimraf": "^2.6.1",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  }
}
